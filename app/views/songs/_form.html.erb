<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.display_name(@song, @path) %>
  <%if !params[:artist_id]%>
  <%= f.collection_check_boxes(:song, :artist_ids, Artist.all, :id, :name) %><br><%end%>

  <%= f.submit %>
<% end %>